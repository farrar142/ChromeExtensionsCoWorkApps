    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.6.1",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "latest",
    "@mui/system": "^5.6.0",
    "axios": "latest",
    "next": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "recoil": "^0.6.1",
