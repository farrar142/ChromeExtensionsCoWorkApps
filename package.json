{
  "name": "nextjs",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dist": "next build && next start",
    "build": "next build",
    "extension": "next build && next export && mv out/_next out/assets && sed -i 's/\\/_next/\\/assets/g' out/**.html",
